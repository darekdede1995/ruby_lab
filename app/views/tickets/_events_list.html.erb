<h1>Wydarzenia</h1>

<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Id</th>
      <th>Description</th>
      <th>Price min</th>
      <th>Price max</th>
      <th>Date </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= event.artist %></td>
        <td><%= event.id %></td>
        <td><%= event.description %></td>
        <td><%= event.price_low %></td>
        <td><%= event.price_high %></td>
        <td><%= event.event_date %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>
